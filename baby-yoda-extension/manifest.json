{
  "manifest_version": 2,
  "name": "babyYoda Much?",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_title": "Baby Yodaized"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js","content.js"]
    }
  ],
  "web_accessible_resources": [
    "dogyoda.jpeg"
  ]
}
